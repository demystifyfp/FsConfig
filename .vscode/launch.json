    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "dotnet",
            "args": [
                "test"
            ],
            "cwd": "${workspaceFolder}/tests/FsConfig.Tests",
            "stopAtEntry": false,
            "console": "internalConsole",
            "env": {
                "VSTEST_HOST_DEBUG": "1"
            }
        }
